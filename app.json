{
  "expo": {
    "name": "Bakesale",
    "entryPoint": "index.js",
    "slug": "bakesale",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bakesale-logo.png",
    "splash": {
      "image": "./assets/bakesale-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.example.bakesale",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.example.bakesale",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
